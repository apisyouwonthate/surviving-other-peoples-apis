{
  "$id": "http://example.com/schemas/user.json",
  "type": "object",
  "definitions": {},
  "$schema": "http://json-schema.org/draft-06/schema#",
  "properties": {
    "name": {
      "type": "string",
      "maxLength": 20
    },
    "email": {
      "type": "string",
      "format": "email"
    },
    "registered_date": {
      "type": "string",
      "format": "date-time",
      "readOnly": true
    }
  },
  "required": ["name"]
}
